{
    "name": "rapid-snippets",
    "displayName": "Rapid-Snippets",
    "description": "Fast Code Generator - Basically This Extension Generate Predefine Snippets for starting new project that will be help you to rapidly developed project.",
    "version": "1.0.0",
    "publisher": "Harshil-Kaneria",
    "repository": {
      "type": "git",
      "url": "https://github.com/Harshil-Kaneria/VS-Code-Rapid-Snippets"  
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/html.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/express_router.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/react_module.code-snippets"
            }
        ]
    }
}
