    "React-JS-Class": {
        "prefix": "!rjc",
        "body": [
            "import React from 'react';",
            "import './App.css';",
            "$0",
            "// Make Class",
            "class App extends React.Component {",
            "  render() {",
            "    return (<><p>${1:Wow}</p></>);",
            "  }",
            "}",
            "$0",
            "// Export Class For Use in Another File",
            "export default App;"
        ],
        "description": "Generate React-Js Template with Class  exports"
